<div class="container">
  <div class="row">
    <div class="col">
      <h2>魚一覧</h2>
      <table>
        <tr>
          <td>魚名</td>
        </tr>
        
        <% @fish.each do |fish| %>
        <tr>
          <td><%= link_to admin_fish_path(fish.id) do %>
              <%= fish.fish_name %>
              <% end %>
          </td>
        </tr>
        <% end %>
      </table>
      <%= link_to '追加', new_admin_fish_path, class:"btn btn-success" %>
    </div>
  </div>
</div>
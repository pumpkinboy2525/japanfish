<div class="container">
  <div class="row">
    <div class="col">
      <h2 class="my-4">新規登録</h2>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
 　    <%= render "public/shared/error_messages", resource: resource %>
 　    
 　    <table>
 　      <tr>
 　        <td>名前</td>
 　        <td>(姓)</td>
 　        <td><%= f.text_field :last_name, autofocus: true, autocomplete: "last_name" %></td>
 　        <td>(名)</td>
 　        <td><%= f.text_field :first_name, autocomplete: "first_name" %></td>
 　      </tr>
 　      
 　      <tr>
 　        <td>フリガナ</td>
 　        <td>(セイ)</td>
 　        <td><%= f.text_field :last_name_kana, autocomplete: "last_name_kana" %></td>
 　        <td>(メイ)</td>
 　        <td><%= f.text_field :first_name_kana, autocomplete: "first_name_kana" %></td>
 　      </tr>
 　      
 　      <tr>
 　        <td>メールアドレス</td>
 　        <td></td>
 　        <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
 　        <td></td>
 　        <td></td>
 　      </tr>
 　      
 　      <tr>
 　        <td>パスワード(6字以上)</td>
 　        <td></td>
 　        <td><%= f.password_field :password, autocomplete: "new-password" %></td>
 　        <td></td>
 　        <td></td>
 　      </tr>
 　      
 　      <tr>
 　        <td>パスワード(確認)</td>
 　        <td></td>
 　        <td><%= f.password_field :password_confirmation, autocomplete: "new-password" %></td>
 　        <td></td>
 　        <td></td>
 　      </tr>
 　    </table>
 　    <div><%= f.submit "新規登録" %></div> 
 　    <% end %>
    </div>
  </div>
</div>
